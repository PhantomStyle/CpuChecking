<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Main">
   <children>
      <LineChart fx:id="romLineChart" layoutX="99.0">
        <xAxis>
          <CategoryAxis side="BOTTOM" />
        </xAxis>
        <yAxis>
          <NumberAxis side="LEFT" />
        </yAxis>
      </LineChart>
      <Button fx:id="cpuButton" layoutY="2.0" mnemonicParsing="false" onAction="#onCpuButton" prefHeight="58.0" prefWidth="99.0" text="Cpu" />
      <Button fx:id="romButton" layoutY="81.0" mnemonicParsing="false" onAction="#onRomButton" prefHeight="58.0" prefWidth="99.0" text="Rom" />
      <LineChart fx:id="cpuLineChart" layoutX="99.0">
         <xAxis>
            <CategoryAxis side="BOTTOM" />
         </xAxis>
         <yAxis>
            <NumberAxis side="LEFT" />
         </yAxis>
      </LineChart>
   </children>
</AnchorPane>
